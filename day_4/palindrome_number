class Solution {
    public boolean isPalindrome(int x) {
        int power = 10;
        int res = 0;
        int n = x;
        while(x>0){
            int r = x%10;
            x=x/10;
            res=res*10+r; 
        }
        
        if(res==n){
            return true;
        } 
        return false;
    }
}